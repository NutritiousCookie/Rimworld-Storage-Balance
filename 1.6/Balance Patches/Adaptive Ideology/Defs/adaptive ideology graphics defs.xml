<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!-- GRAPHICS -->
    <!-- freight -->
    <AdaptiveStorage.GraphicsDef ParentName="Freight_A_BaseGraphics">
        <defName>Freight_A_Chunk</defName>
        <useDominantContentColor>true</useDominantContentColor>
        <graphics>
            <li>
                <graphicData>
                    <texPath>Chunk/Chunk_A</texPath>
                    <graphicClass>Graphic_Single</graphicClass>
                    <drawSize>(5,2.95)</drawSize>
                    <drawRotated>false</drawRotated>
                </graphicData>
                <minimumStackCount>1</minimumStackCount>
            </li>
        </graphics>
        <onlyAllowRequiredThingDefs>true</onlyAllowRequiredThingDefs>
        <allowedThingCategories>
            <li>Chunks</li>
        </allowedThingCategories>
    </AdaptiveStorage.GraphicsDef>

