<!-- CONFIGURE STORAGE -->
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[defName="Vin_DLR" or defName="Vin_GR" or defName="Vin_GRS" or defName="Vin_TLR" or defName="Vin_CB" or defName="Vin_CBS" or defName="Vin_PC"]/building</xpath>
        <value>
            <fixedStorageSettings>
                <filter>
                    <categories>
                        <li>Root</li>
                    </categories>
                    <disallowedCategories>
                        <li>Chunks</li>
                        <li>Corpses</li>
                    </disallowedCategories>
                </filter>
            </fixedStorageSettings>
        </value>
    </Operation>

<!-- FERNY ORGANIZE -->
    <Operation Class="PatchOperationConditional">
        <xpath>Defs/DesignationCategoryDef[defName="Ferny_Storage"]</xpath>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="Vin_DLR" or defName="Vin_GR" or defName="Vin_GRS" or defName="Vin_TLR"]</xpath>
                    <value>
                        <designationCategory>Ferny_GeneralStorage</designationCategory>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="Vin_CB" or defName="Vin_CBS" or defName="Vin_MR" or defName="Vin_MRS" or defName="Vin_PC"]</xpath>
                    <value>
                        <designationCategory>Ferny_GearStorage</designationCategory>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="Vin_FR"]</xpath>
                    <value>
                        <designationCategory>Ferny_FoodStorage</designationCategory>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>

<!-- REMOVE ITEMS -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Reel's Expanded Storage</li>
            <li>Phaneron's Basic Storage</li>
            <li>[sbz] Neat Storage</li>
            <li>LWM's Adaptive Deep Storage</li>
            <li>Adaptive Simple Storage</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="Vin_CB" or defName="Vin_CBS" or defName="Vin_MR" or defName="Vin_MRS" or defName="Vin_PC"]</xpath>
                    <value>
                        <designationCategory Inherit="False"></designationCategory>
                    </value>
                </li>
                <li Class="PatchOperationAdd" MayRequire="oskarpotocki.vanillafactionsexpanded.core">
                    <xpath>Defs/VEF.Buildings.HiddenDesignatorsDef[defName="StorageBalancePatches_HiddenDesignators"]/hiddenDesignators</xpath>
                    <value>
                        <li>Vin_CB</li>
                        <li>Vin_CBS</li>
                        <li>Vin_MR</li>
                        <li>Vin_MRS</li>
                        <li>Vin_PC</li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>