<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- REMOVE RESEARCH -->
<!-- ITEMS -->

    <Operation Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageAnimalFeeder" or defName="ReelStorageNewBarrel" or defName="ReelStorageNewWoodCrate" or defName="ReelStorageNewLargeWoodCrate" or defName="ReelStorageBookshelf" or defName="ReelStorageWineRack" or defName="ReelStorageWoodChest" or defName="ReelStorageLargeWoodChest"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>ComplexFurniture</li>
                </researchPrerequisites>
            </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageNewFridge" or defName="ReelStorageNewMedicineCabinet"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>AirConditioning</li>
                </researchPrerequisites>
            </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageNewLargeLocker" or defName="ReelStorageSkip" or defName="ReelStorageMetalCrate" or defName="ReelStorageLargeMetalCrate" or defName="ReelStorageVerticalWeaponRack" or defName="ReelStorageHorizontalWeaponRack" or defName="ReelStorageCargoContainer"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>Machining</li>
                </researchPrerequisites>
            </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageMeatHook"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>Smithing</li>
                </researchPrerequisites>
            </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageBarrelPallet"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>BiofuelRefining</li>
                </researchPrerequisites>
            </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageStonePile" or defName="ReelStorageLargeStonePile"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>Stonecutting</li>
                </researchPrerequisites>
            </value>
    </Operation>
    <Operation Class="PatchOperationRemove">
            <xpath>Defs/ThingDef[defName="ReelStorageWoodPile"]/researchPrerequisites</xpath>
    </Operation>
    <Operation Class="PatchOperationRemove">
            <xpath>Defs/ThingDef[defName="ReelStorageLargeWoodPile"]/researchPrerequisites</xpath>
    </Operation>

    <!-- Vanilla Furniture Expanded Patches -->
    <!-- Core -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="ReelNewIndustrialShelf"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>MF_ModernFurniture</li>
                        </researchPrerequisites>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="Table_Wardrobe"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>ComplexFurniture</li>
                        </researchPrerequisites>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="ReelStorageTanningRack" or defName="ReelStorageWeaponRack"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>MF_BasicFurniture</li>
                        </researchPrerequisites>
                    </value>
                </li>
            </operations>
        </match>
        <nomatch Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="ReelNewIndustrialShelf"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>Machining</li>
                        </researchPrerequisites>
                    </value>
                </li>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/ThingDef[defName="ReelStorageTanningRack or defName="ReelStorageWeaponRack"]/researchPrerequisites</xpath>
                </li>
            </operations>
        </nomatch>
    </Operation>
    <!-- Farming -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Farming</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="ReelStorageHayBale"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>VFE_Res_FarmingTechniques</li>
                        </researchPrerequisites>
                    </value>
                </li>
            </operations>
        </match>
        <nomatch Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="ReelStorageHayBale"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>ComplexFurniture</li>
                        </researchPrerequisites>
                    </value>
                </li>
            </operations>
        </nomatch>
    </Operation>
    <!-- Spacer -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Spacer Module</li>
        </mods>
        <match  Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageLargeSpacerContainer" or defName="ReelStorageSpacerContainer" or defName="ReelStorageSpacerContainerStack" or defName="ReelStorageSpacerLocker"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>SpacerElectronics</li>
                </researchPrerequisites>
            </value>
        </match>
        <nomatch Class="PatchOperationReplace">
            <xpath>Defs/ThingDef[defName="ReelStorageLargeSpacerContainer" or defName="ReelStorageSpacerContainer" or defName="ReelStorageSpacerContainerStack" or defName="ReelStorageSpacerLocker"]/researchPrerequisites</xpath>
            <value>
                <researchPrerequisites>
                    <li>ShipBasics</li>
                </researchPrerequisites>
            </value>
        </nomatch>
    </Operation>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                <xpath>Defs/ThingDef[defName="ReelStorageAnimalFeeder" or defName="ReelStorageNewBarrel" or defName="ReelStorageNewWoodCrate" or defName="ReelStorageNewLargeWoodCrate" or defName="ReelStorageBookshelf" or defName="ReelStorageWineRack" or defName="ReelStorageWoodChest" or defName="ReelStorageLargeWoodChest"]/researchPrerequisites</xpath>
                <value>
                    <researchPrerequisites>
                        <li>DankPyon_RusticStorage</li>
                    </researchPrerequisites>
                </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="ReelStorageHayBale"]/researchPrerequisites</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>DankPyon_BasicAgriculture</li>
                        </researchPrerequisites>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>

<!-- PROJECTS -->
    <Operation Class="PatchOperationConditional">
        <xpath>Defs/ResearchProjectDef[defName="ReelNeolithicStorage"]</xpath>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationConditional">
                    <xpath>Defs/ThingDef/researchPrerequisites[li="ReelNeolithicStorage"]</xpath>
                    <nomatch Class="PatchOperationRemove">
                        <xpath>Defs/ResearchProjectDef[defName="ReelNeolithicStorage"]</xpath>
                    </nomatch>
                </li>
                <li Class="PatchOperationConditional">
                    <xpath>Defs/ThingDef/researchPrerequisites[li="ReelMedievalStorage"]</xpath>
                    <nomatch Class="PatchOperationRemove">
                        <xpath>Defs/ResearchProjectDef[defName="ReelMedievalStorage"]</xpath>
                    </nomatch>
                </li>
            </operations>
        </match>
    </Operation>
    <Operation Class="PatchOperationConditional">
        <xpath>Defs/ThingDef/researchPrerequisites[li="ReelIndustrialStorage"]</xpath>
        <nomatch Class="PatchOperationRemove">
            <xpath>Defs/ResearchProjectDef[defName="ReelIndustrialStorage"]</xpath>
        </nomatch>
    </Operation>
    <Operation Class="PatchOperationConditional">
        <xpath>Defs/ThingDef/researchPrerequisites[li="ReelSpacerStorage"]</xpath>
        <nomatch Class="PatchOperationRemove">
            <xpath>Defs/ResearchProjectDef[defName="ReelSpacerStorage"]</xpath>
        </nomatch>
    </Operation>
</Patch>