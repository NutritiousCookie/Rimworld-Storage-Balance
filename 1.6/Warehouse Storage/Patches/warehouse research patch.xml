<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- ITEMS -->
    <Operation Class="PatchOperationConditional">
        <xpath>ModSettings[Class="StorageBalance.StorageBalanceSettings"]/doStorageResearch</xpath>
        <nomatch Class="PatchOperationSequence">
            <operations>
            <!-- ResearchLevelDef -->
                <li Class="StorageBalance.PatchOperationStorageResearch">
                    <xpath>Defs/ThingDef[defName="Vin_CB" or defName="Vin_CBS" or defName="Vin_DLR" or defName="Vin_GR" or defName="Vin_GRS" or defName="Vin_PC" or defName="Vin_TLR" or defName="Vin_WP" or defName="Vin_WPS"]</xpath>
                    <techLevel>AdvancedIndustrialStorage</techLevel>
                </li>
                <li Class="StorageBalance.PatchOperationStorageResearch">
                    <xpath>Defs/ThingDef[defName="Vin_MR" or defName="Vin_MRS"]</xpath>
                    <techLevel>IndustrialStorage</techLevel>
                </li>
            <!-- ResearchPrerequisites -->
                <li Class="PatchOperationReplace">
                        <xpath>Defs/ThingDef[defName="Vin_FR"]/researchPrerequisites</xpath>
                        <value>
                            <researchPrerequisites>
                                <li>AirConditioning</li>
                            </researchPrerequisites>
                        </value>
                </li>
            </operations>
        </nomatch>
    </Operation>
</Patch>